{
  "include": ["declarations.d.ts"],
  "exclude": [],
  "files": [],
  "references": [
    { "path": "./packages/adapters" },
    { "path": "./packages/core" },
    { "path": "./packages/react" },
    { "path": "./packages/solid" },
    { "path": "./packages/preact" },
    { "path": "./packages/vue" }
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "commonjs",
    "target": "ES2022",
