{
  "include": [
    "src",
    "build.ts"
  ],
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "noEmit": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "paths": {
      "reactive-route": ["../../packages/core/index.ts"],
      "reactive-route/adapters/*": ["../../packages/adapters/*"]
    }
  }
}
